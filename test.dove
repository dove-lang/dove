class Person {
    fun init(name, friends) {
        self.name = name
        self.friends = friends
    }

    fun get_friend(name) {
        for friend in self.friends {
            if friend.name == name {
                return friend
            }
        }

        return nil
    }
}

let a = Person("a", [])
let b = Person("b", [])
let c = Person("c", [a, b])

print c.get_friend("b").name

c.friends[1] = a

print c.get_friend("b")
print c.get_friend("a").name
